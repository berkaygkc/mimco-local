<style>
    .hidden {
        display: none
    }
</style>

<script>
    $(document).ready(function () {
        if ('{{allJson.ProfileID}}' == 'EARSIVFATURA') {
            document.querySelector('#invoice-profile-select').setDisabledOptions(['TEMELFATURA', 'TICARIFATURA']);
        } else {
            document.querySelector('#invoice-profile-select').setDisabledOptions(['EARSIVFATURA']);
        }
        document.querySelector('#invoice-profile-select').setValue('{{allJson.ProfileID}}');

        setTimeout(
            () => {
                document.querySelector('#invoice-type-select').setValue('{{allJson.InvoiceType}}');
                document.querySelector('#kdv-muaf-select').setValue('{{allJson.TaxExemptionReasonCodeGeneral}}');
                document.querySelector('#export-termcode-select').setValue('{{allJson.ExportInfo.TermCode}}'); document.querySelector('#export-transportmode-select').setValue('{{allJson.ExportInfo.TransportMode}}');
                if ('{{allJson.ExportInfo.Delivery}}') {
                    document.getElementById("delivery_address").value = '{{allJson.ExportInfo.Delivery.Address}}';
                    document.getElementById("delivery_district").value = '{{allJson.ExportInfo.Delivery.District}}';
                    document.getElementById("delivery_city").value = '{{allJson.ExportInfo.Delivery.City}}';
                    document.getElementById("delivery_country").value = '{{allJson.ExportInfo.Delivery.Country}}';
                    document.getElementById("delivery_postalcode").value = '{{allJson.ExportInfo.Delivery.PostalCode}}';
                } else {
                    document.getElementById("delivery_address").value = '{{customer.Address}}';
                    document.getElementById("delivery_district").value = '{{customer.District}}';
                    document.getElementById("delivery_city").value = '{{customer.City}}';
                    document.getElementById("delivery_country").value = '{{customer.Country}}';
                    document.getElementById("delivery_postalcode").value = '{{customer.PostalCode}}';
                }
            }
            , 500);
    });
</script>

{{/if}}